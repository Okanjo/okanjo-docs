<!doctype html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.png">

    <title>{{ page.title }}</title>

    <link  href='css/theme.css' rel='stylesheet'>

    <!-- Meta -->
    <meta content="{{ page.title|e }}" property="og:title">
    <meta content="{{ page.description|e }}" name="description">

    <!-- Initializer -->

{% block head %}
{% endblock %}

</head>
<body role='flatdoc'>

<div class="okanjo-header">
    <a href="/"><img src="img/okanjo.png"></a><h1>Developer Docs</h1>
</div>

<div class='header'>
    {% block header %}
    {% endblock %}
</div>

{% block childrenHeader %}{% endblock %}

<div class='content-root'>
{% block content %}
{% endblock %}
</div>
